<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line">
    <!--显示一条虚线，破折线的宽度为dashWith，破折线之间的空隙的宽度为dashGap，当dashGap=0dp时，为实线 -->
    <stroke
        android:dashGap="3dp"
        android:dashWidth="6dp"
        android:width="1dp"
        android:color="#d2d2d7"/>
    <!-- 虚线的高度 -->
    <size android:height="1dp"/>
</shape>